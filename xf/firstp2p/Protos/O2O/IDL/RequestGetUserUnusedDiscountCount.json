{
  "author": "yanbingrong",
  "comment": "获取用户可赠送的投资券个数",
  "namespace": "NCFGroup\\Protos\\O2O",
  "use": ["NCFGroup\\Common\\Extensions\\Base\\AbstractRequestBase", "Assert\\Assertion"],
  "extend": "AbstractRequestBase",
  "properties": [
    {
      "name": "userId",
      "comment": "用户ID",
      "type": "int",
      "required": true
    },
    {
      "name": "type",
      "comment": "投资券类型，1为返现券，2为加息券，3为黄金券，0不区分",
      "type": "int",
      "required": false,
      "default": 0
    },
    {
      "name": "consumeType",
      "comment": "交易类型，1为p2p，2为duotou，3为gold",
      "type": "int",
      "required": false,
      "default": 1
    }
  ]
}
