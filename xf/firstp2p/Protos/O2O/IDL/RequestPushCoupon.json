{
    "author": "liguizhi",
    "comment": "o2o:向合作方推送券信息",
    "namespace": "NCFGroup\\Protos\\O2O",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "orderId",
            "comment": "优惠券兑换订单",
            "type": "string",
            "required": true
        },
        {
            "name": "clientId",
            "comment": "网信给合作方分配的唯一key",
            "type": "string",
            "required": true
        },
        {
            "name": "beginTime",
            "comment": "优惠券开始时间",
            "type": "int",
            "required": true
        },
        {
            "name": "endTime",
            "comment": "优惠券结束时间",
            "type": "int",
            "required": true
        },
        {
            "name": "price",
            "comment": "优惠券金额",
            "type": "string",
            "required":false,
            "default" : ""
        },
        {
            "name": "productId",
            "comment": "合作方商品编号",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "userId",
            "comment": "网信userId",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "phone",
            "comment": "网信用户绑定的手机号",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "userName",
            "comment": "合作方用户名",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "email",
            "comment": "合作方用户邮箱",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "idno",
            "comment": "合作方用户身份证号码",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "extra",
            "comment": "合作方其他信息",
            "type": "string",
            "required": false,
            "default": ""
        }
    ]
}
