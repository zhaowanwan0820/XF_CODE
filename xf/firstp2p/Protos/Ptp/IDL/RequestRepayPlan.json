{
    "author": "yutao",
    "comment": "用户回款计划Request",
    "namespace": "NCFGroup\\Protos\\Ptp",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\AbstractRequestBase", "Assert\\Assertion"],
    "extend": "AbstractRequestBase",
    "properties": [
        {
            "name": "userId",
            "comment": "用户ID",
            "type": "int",
            "required": true
        },
        {
            "name": "beginTime",
            "comment": "起始时间",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "endTime",
            "comment": "结束时间",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "type",
            "comment": "还款类型",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "count",
            "comment": "每页数量",
            "type": "int",
            "required": false,
            "default": 10
        },
        {
            "name": "offset",
            "comment": "起始位置",
            "type": "int",
            "required": false,
            "default": 0
        }
    ]
}

