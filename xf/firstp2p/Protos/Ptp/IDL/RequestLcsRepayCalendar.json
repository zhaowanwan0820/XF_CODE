{
    "author": "yutao",
    "comment": "理财师客户相关",
    "namespace": "NCFGroup\\Protos\\Ptp",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "cfpId",
            "comment": "理财师ID",
            "type": "int",
            "required": true
        },
        {
            "name": "repayType",
            "comment": "还款类型",
            "type": "string",
            "required": false,
            "default": "alRepay"
        },
        {
            "name": "date",
            "comment": "时间类型2016-02-03",
            "type": "string",
            "required": true
        },
        {
            "name": "count",
            "comment": "数量",
            "type": "int",
            "required": false,
            "default": 10
        },
        {
            "name": "offset",
            "comment": "偏移量",
            "type": "int",
            "required": false,
            "default": 0
        }
    ]
}

