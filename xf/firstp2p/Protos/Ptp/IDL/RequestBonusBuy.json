{
    "author": "zhangzhuyan",
    "comment": "理财师用户买红包",
    "namespace": "NCFGroup\\Protos\\Ptp",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\AbstractRequestBase", "Assert\\Assertion"],
    "extend": "AbstractRequestBase",
    "properties": [
        {
            "name": "userId",
            "comment": "用户ID",
            "type": "int",
            "required": true
        },
        {
            "name": "totalPrice",
            "comment": "总金额",
            "type": "float",
            "required": true
        },
        {
            "name": "count",
            "comment": "红包个数",
            "type": "int",
            "required": true
        },
        {
            "name": "isRandom",
            "comment": "是否随机",
            "type": "boolean",
            "required": true
        },
        {
            "name": "receiveMode",
            "comment": "领取模式(0:无限制，1:检查所属关系)",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "showLcs",
            "comment": "是否显示理财师姓名",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "orderID",
            "comment": "订单号",
            "type": "string",
            "required": true
        },
        {
            "name": "sign",
            "comment": "签名",
            "type": "string",
            "required": true
        },
        {
            "name": "timestamp",
            "comment": "时间戳",
            "type": "string",
            "required": true
        },
        {
            "name": "clientID",
            "comment": "客户端ID",
            "type": "string",
            "required": true
        }
    ]
}

