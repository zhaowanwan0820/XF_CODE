{
    "namespace": "NCFGroup\\Protos\\Ptp",
    "comment": "第三方交互-单笔订单查询接口请求定义",
    "author": "郭峰 <guofeng3@ucfgroup.com>",
    "use": {
        "0": "NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase",
        "1": "Assert\\Assertion"
    },
    "extend": "ProtoBufferBase",
    "properties": {
        "0": {
            "required": true,
            "name": "merchantId",
            "type": "int",
            "comment": "商户ID"
        },
        "1": {
            "required": false,
            "name": "outOrderId",
            "type": "string",
            "comment": "外部订单号",
            "default": ""
        },
        "2": {
            "required": false,
            "name": "startTime",
            "type": "int",
            "comment": "查询开始时间戳",
            "default": 0
        },
        "3": {
            "required": false,
            "name": "endTime",
            "type": "int",
            "comment": "查询结束时间戳",
            "default": 0
        },
        "4": {
            "required": false,
            "name": "pageNo",
            "type": "int",
            "comment": "当前页码",
            "default": 1
        },
        "5": {
            "required": false,
            "name": "pageLimit",
            "type": "int",
            "comment": "每页记录数",
            "default": 100
        }
    }
}