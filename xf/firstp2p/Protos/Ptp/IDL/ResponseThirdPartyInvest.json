{
    "namespace": "NCFGroup\\Protos\\Ptp",
    "comment": "第三方交互-投资接口返回定义",
    "author": "guofeng3",
    "use": {
        "0": "NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase",
        "1": "Assert\\Assertion"
    },
    "extend": "ProtoBufferBase",
    "properties": {
        "0": {
            "required": true,
            "name": "outOrderId",
            "type": "string",
            "comment": "众筹付款单号"
        },
        "1": {
            "required": true,
            "name": "respCode",
            "type": "string",
            "comment": "00业务受理成功，01业务受理失败"
        },
        "2": {
            "required": false,
            "name": "respMsg",
            "type": "string",
            "comment": "受理失败原因",
            "default": ""
        },
        "3": {
            "required": true,
            "name": "orderStatus",
            "type": "string",
            "comment": "付款单状态，N(初始状态)I(处理中)S(成功)F(失败)"
        }
    }
}