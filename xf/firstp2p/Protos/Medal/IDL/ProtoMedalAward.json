{
    "author": "luzhengshuai",
    "comment": "新手勋章medal对应奖励",
    "namespace": "NCFGroup\\Protos\\Medal",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "id",
            "comment": "奖励ID",
            "type": "int",
            "required": true
        },
        {
            "name": "medalId",
            "comment": "奖励所属勋章ID",
            "type": "int",
            "required": true
        },
        {
            "name": "timeStart",
            "comment": "奖励开始时间",
            "type": "int",
            "required": true
        },
        {
            "name": "awardList",
            "comment": "奖励列表（券组ID列表)",
            "type": "string",
            "required": true
        },
        {
            "name": "awardNum",
            "comment": "奖励可领取数量",
            "type": "int",
            "required": true
        },
        {
            "name": "awardLimitTime",
            "comment": "奖励领取时间限制",
            "type": "int",
            "required": true,
            "default": 0
        },
        {
            "name": "inviterAwardList",
            "comment": "邀请人奖励",
            "type": "string",
            "required": false,
            "default": ""
        }
    ]
}
