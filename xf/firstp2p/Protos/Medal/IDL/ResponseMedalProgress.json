{
    "author": "dengyi",
    "comment": "新手任务进度",
    "namespace": "NCFGroup\\Protos\\Medal",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "isBeginner",
            "comment": "是否是新手",
            "type": "bool",
            "required": true
        },
        {
            "name": "beginnerMedalCount",
            "commnet": "新手勋章数量",
