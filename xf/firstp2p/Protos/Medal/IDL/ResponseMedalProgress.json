            "required": false,
            "default": 0
        },
        {
            "name": "userBeginnerMedalCount",
            "commnet": "用户获得的新手勋章数量",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "unawardedBeginnerMedals",
            "commnet": "未领奖的新手勋章列表",
            "type": "array",
            "required": false,
            "default": null
        },
        {
            "name": "unawardedMedals",
            "comment": "未领奖的勋章列表",
            "type": "array",
            "required": false,
            "default": null
        }
    ]
