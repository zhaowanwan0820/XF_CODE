@charset "utf-8";
body {background:#fff;}
.main {width:1200px;}
.icon-finyear {cursor: pointer;}
.p_index_bg {background: url(../../images/index/5@2x.png) no-repeat center ;}

.p_index .banner_slide ul.banner_view ,.p_index .banner_slide,.p_index .banner_slide ul.banner_view li {height:660px;}
.p_index .banner_slide ul.banner_view li {background-size: cover;}

.infor_list {background: #fff url(../../images/index/25@2x.png) no-repeat left center ;}
.infor_list li {float:left;width:280px;height:130px;line-height:25px;padding-right:20px;padding-top:35px;background: url(../../images/index/26@2x.png) no-repeat right center;  }
.infor_list li div {text-align: center;}
.infor_list li h5 {font-size:16px;}
.infor_list li h6 {font-size:18px;}
.infor_list .bg2 {background-image: url(../../images/index/27@2x.png); }
.infor_list .bg3 {background-image: url(../../images/index/28@2x.png); }
.infor_list .bg4 {background-image: url(../../images/index/25@2x.png); }

.station_intro {height:100px;margin:25px 0 70px;padding-left:300px;background: url(../../images/index/54@2x.png) no-repeat left center;padding-top:20px; }
.station_intro h5 {width:632px;line-height:30px;font-size:16px;}
.station_intro .fr {width:100px;height:40px;border:1px solid #ccc;border-radius: 20px;margin-top:20px;font-size:16px;line-height: 40px;text-align: center;}

.group-summary {padding-top:30px;padding-bottom:70px;}
.group-summary .title {text-align: center;}
.history #year div ul li.cur {color:#FC7F0C;}
.history #year a.prev.cur { }
.history #year a.prev { }
.history #year a.next.cur {background-image: url(../../images/index/@2x1.png);}

.infor_descrip {border-top: 1px solid #F57A10;}
.infor_descrip dl {width: 600px;margin:0 auto;font-size: 16px;color:#333;margin-top:90px;}
.infor_descrip dt ,.infor_descrip dd {float:left;}
.infor_descrip dt {width:150px;height:50px;background: url(../../images/index/8@2x.png) no-repeat right center;font-size: 48px;color:#FC7F0C;text-align: center;line-height:50px;}
.infor_descrip dd {padding-left:50px;width:400px;}
.history #content .txt {background: none;}
.history #content {margin-top:100px;}
.history #content:before {background: none;}

.p_index_bg2 {background: url(../../images/index/32@2x.png) no-repeat center ;height: 485px;padding-top:70px;}
.p_index_bg2 ul {margin-left:100px;color:#fff;}
.p_index_bg2 ul li{width:12px;float:left;font-size:12px;margin-right:20px;line-height: 15px;}
.p_index_bg2 h4 {margin-left:95px;}
.p_index_bg2 h5 {width:49px;height:245px;background: url(../../images/index/20180330135533.png) no-repeat 0 center;}
.p_index_bg2 h6 {width:610px;text-align: right;font-style: italic;border-bottom: 1px solid #fff;color:#fff;float:right;font-size: 30px;margin-top:30px;letter-spacing: 20px;padding-bottom:8px;}

.main_part1 {padding:80px 0  50px;min-height:300px;background: #fff;background: url(../../images/index/355@2x.png) no-repeat 100px 80px;font-size: 24px;}
.main_part1 .infor{width:710px;border:5px solid #FFC61A;min-height: 150px;border-radius: 30px;text-align: center;margin:0 auto;color:#333;padding-top:15px;}
.main_part1 .infor h4 span {color:#F75938;font-size: 30px;}
.main_part1 .infor h4 em {color:#F75938;}
.main_part1 .infor h3 {margin-top: 20px;}
.main_part1 .infor h4 {}
.main_part1 .xinzai {width:103px;height:72px;background: url(../../images/index/40@2x.png) no-repeat 0px 0px;margin:0 auto;position: relative;top:46px;}
.main_part1 .invest_btn a {display: block;width:508px;height:65px;border-radius: 30px;background: #F75938;line-height: 65px;text-align: center;color:#FFF4D3;margin: 41px auto;}
.main_part1 span.fr {margin-top:262px;font-size:16px;}
.main_part1 span.fr em {color:#FC7F0C;}

.main_part2 {padding:100px 0 ;border-top:1px solid #ccc; background: url(../../images/index/551@2x.png) no-repeat center 100px;}
.main_part2 .nav_menu {width:350px;margin: 150px auto 100px;}
.main_part2 .nav_menu li {float:left;color:#333;font-size:18px;height:35px;line-height:35px;}
/*common*/

