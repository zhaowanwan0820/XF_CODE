{
    "author": "fanjingwen",
    "comment": "更改项目信息中的银行卡信息",
    "namespace": "NCFGroup\\Protos\\Ptp",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "approveNumber",
            "comment": "放款审批单编号",
            "type": "string",
            "required": true
        },
        {
            "name": "bankcard",
            "comment": "新的银行卡账号",
            "type": "string",
            "required": true
        },
        {
            "name": "bankId",
            "comment": "银行",
            "type": "int",
            "required": true
        },
        {
            "name": "bankZone",
            "comment": "开户网点",
            "type": "string",
            "required": true
        },
        {
            "name": "cardName",
            "comment": "开户人姓名",
            "type": "string",
            "required": true
        },
        {
            "name": "cardType",
            "comment": "放款账号类型(1:对公 0:对私)",
            "type": "int",
            "required": true
        },
        {
            "name": "clearingType",
            "comment": "交易所结算方式(1:场内 2:场外)",
            "type": "int",
            "required": false,
            "default": 0
        }
    ]
}

