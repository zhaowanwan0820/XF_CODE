{
    "namespace": "NCFGroup\\Protos\\Ptp",
    "comment": "开发平台获取优惠码接口",
    "author": "milining",
    "use":[
        "NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase",
        "Assert\\Assertion"
    ],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "required": true,
            "name": "userId",
            "type": "int",
            "comment": "用户id"
        },
        {
            "required": false,
            "name": "type",
            "type": "string",
            "comment": "查询类型：p2p，多投宝,reg",
            "default": "p2p"
        },
        {
            "required": false,
            "name": "siteId",
            "type": "int",
            "comment": "站点id",
            "default": 0
        },
        {
            "required": false,
            "name": "payStatus",
            "type": "string",
            "comment": "结算状态（参加CouponService常量，6:线下结算; all:所有）",
            "default": "all"
        },
        {
            "required": false,
            "name": "payTimeStart",
            "type": "int",
            "comment": "结算时间起点（包括）",
            "default": 0
        },
        {
            "required": false,
            "name": "payTimeEnd",
            "type": "int",
            "comment": "结算时间终点（包括）",
            "default": 0
        },
        {
            "required": false,
            "name": "getResType",
            "type": "string",
            "comment": "获取结果的类型(stat/list/all)",
            "default": "all"
        },
        {
            "required": false,
            "name": "createTimeStart",
            "type": "int",
            "comment": "创建时间起点（包括）",
            "default": 0
        },
        {
            "required": false,
            "name": "createTimeEnd",
            "type": "int",
            "comment": "创建时间终点（包括）",
            "default": 0
        },
        {
            "required": false,
            "name": "pageNum",
            "type": "int",
            "comment": "起始页",
            "default": 1
        },
        {
            "required": false,
            "name": "pageSize",
            "type": "int",
            "comment": "页大小",
            "default": 10
        },
        {
            "required": false,
            "name": "mobile",
            "type": "string",
            "comment": "手机号",
            "default": ""
        },
        {
            "required": false,
            "name": "consumeUserId",
            "type": "int",
            "comment": "投资者用户id",
            "default": 0
        }
    ]
}
