{
    "author": "wangyiming",
    "comment": "发送手机短信验证码",
    "namespace": "NCFGroup\\Protos\\Ptp",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "resCode",
            "comment": "结果码",
            "type": "int",
            "required": true
        },
        {
            "name": "errCode",
            "comment": "错误码，默认为0代表成功",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "errMsg",
            "comment": "错误信息",
            "type": "string",
            "required": false,
            "default": null
        }

    ]
}

