{
    "author": "wangyiming",
    "comment": "用户名密码登录，放回用户信息及token",
    "namespace": "NCFGroup\\Protos\\Ptp",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "userId",
            "comment": "用户ID",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "token",
            "comment": "登录token",
            "type": "string",
            "required": false,
            "default": null
        },
        {
            "name": "userInfo",
            "comment": "用户信息",
            "type": "array",
            "required": false,
            "default": null
        },
        {
            "name": "resCode",
            "comment": "结果码",
            "type": "int",
            "required": true
        },
        {
            "name": "errCode",
            "comment": "错误码，默认为0代表成功",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "errMsg",
            "comment": "错误信息",
            "type": "string",
            "required": false,
            "default": null
        }

    ]
}

