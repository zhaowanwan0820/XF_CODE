{
    "namespace": "NCFGroup\\Protos\\Ptp",
    "comment": "发送短信验证码",
    "author": "wangyiming",
    "use":[
        "NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase",
        "Assert\\Assertion"
    ],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "required": true,
            "name": "mobile",
            "type": "string",
            "comment": "手机号码"
        },
        {
            "required": false,
            "name": "countryCode",
            "type": "string",
            "comment": "国家地区码",
            "default": "cn"
        },
        {
            "required": false,
            "name": "invite",
            "type": "string",
            "comment": "注册邀请码",
            "default": null
        }
    ]
}
