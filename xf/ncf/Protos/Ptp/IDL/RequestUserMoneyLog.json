{
    "author": "wangfei5",
    "comment": "资金纪录proto",
    "namespace": "NCFGroup\\Protos\\Ptp",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "userId",
            "comment": "用户ID",
            "type": "int",
            "required": true
        },
        {
            "name": "logInfo",
            "comment": "资金类型",
            "type": "string",
            "required": false,
            "default" : "" 
        },
        {
            "name": "beginTime",
            "comment": "开始时间",
            "type": "int",
            "required": false,
            "default" : 0
        },
        {
            "name": "endTime",
            "comment": "结束时间",
            "type": "int",
            "required": false,
            "default" : 0
        },
        {
            "name": "offset",
            "comment": "纪录偏移量",
            "type": "int",
            "required": false,
            "default" : 0
        },
        {
            "name": "count",
            "comment": "纪录数量",
            "type": "int",
            "required": false,
            "default":20
        },
        {
            "name": "moneyType",
            "comment": "资金记录显示的类型",
            "type": "string",
            "required": false,
            "default" : "money_only"
        }
    ]
}

