{
    "author": "dengyi<dengyi@ucfgroup.com>",
    "comment": "获取勋章列表",
    "namespace": "NCFGroup\\Protos\\Medal",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\AbstractRequestBase", "NCFGroup\\Common\\Extensions\\Base\\Pageable", "Assert\\Assertion"],
    "extend": "AbstractRequestBase",
    "properties": [
        {
            "name": "list",
            "comment": "勋章列表",
            "type": "array",
            "required": true
        },
        {
            "name": "isShowBeginner",
            "comment": "是否显示新手专属",
            "type": "bool",
            "required": false,
            "default": false
        },
        {
            "name": "appPicUrl",
            "comment": "新手专属的APP图片URL",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "webPicUrl",
            "comment": "新手专属的WEB图片URL",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "remainingTime",
            "comment": "新手专属剩余的时间(s)",
            "type": "int",
            "required": false,
            "default": 0
        }
    ]
}
