{
    "author": "luzhengshuai",
    "comment": "medal基本信息",
    "namespace": "NCFGroup\\Protos\\Medal",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\AbstractRequestBase", "Assert\\Assertion"],
    "extend": "AbstractRequestBase",
    "properties": [
        {
            "name": "id",
            "comment": "勋章ID",
            "type": "int",
            "required": true
        },
        {
            "name": "name",
            "comment": "勋章名",
            "type": "string",
            "required": true
        },
        {
            "name": "isLimited",
            "comment": "是否是限量勋章",
            "type": "int",
            "required": true
        },
        {
            "name": "bigLightenedImg",
            "comment": "大的点亮图片的URL",
            "type": "string",
            "required": true
        },
        {
            "name": "mediumLightenedImg",
            "comment": "中等的点亮图片的URL",
            "type": "string",
            "required": true
        },
        {
            "name": "smallLightenedImg",
            "comment": "小的点亮图片的URL",
            "type": "string",
            "required": true
        },
        {
            "name": "bigUnlightenedImg",
            "comment": "大的未点亮图片的URL",
            "type": "string",
            "required": true
        },
        {
            "name": "mediumUnlightenedImg",
            "comment": "中等的未点亮图片的URL",
            "type": "string",
            "required": true
        },
        {
            "name": "smallUnlightenedImg",
            "comment": "小的未点亮图片的URL",
            "type": "string",
            "required": true
        },
        {
            "name": "description",
            "comment": "勋章的描述",
            "type": "string",
            "required": true
        },
        {
            "name": "details",
            "comment": "勋章的详情",
            "type": "string",
            "required": true
        },
        {
            "name": "startTime",
            "comment": "起始时间",
            "type": "int",
            "required": true
        },
        {
            "name": "type",
            "comment": "勋章有效期类型,1.没有结束时期，2.有明确的结束时间(endTime生效)",
            "type": "int",
            "required": true
        },
        {
            "name": "hasAwards",
            "comment": "是否有奖励,1表示没有奖励，2表示有奖励",
            "type": "int",
            "required": true
        },
        {
            "name": "optional",
            "comment": "1表示所有条件必须完成，2表示只要完成条件之一",
            "type": "int",
            "required": true
        },
        {
            "name": "progresses",
            "comment": "勋章的进度",
            "type": "array",
            "required": false,
            "default": null
        },
        {
            "name": "num",
            "comment": "勋章数量",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "endTime",
            "comment": "结束时间",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "isOwned",
            "comment": "用户是否拥有该勋章",
            "type": "bool",
            "required": false,
            "default": false
        },
        {
            "name": "isHistory",
            "comment": "是不是过往的勋章",
            "type": "bool",
            "required": false,
            "default": false
        },
        {
            "name": "isAwarded",
            "comment": "是否已经领奖",
            "type": "bool",
            "required": false,
            "default": false
        },
        {
            "name": "prizeNum",
            "comment": "可领取奖品数量",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "prizeList",
            "comment": "奖品列表，一段json，奖品的id",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "prizeDeadline",
            "comment": "奖品领取的结束时间戳",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "isForBeginner",
            "comment": "是否新手专属",
            "type": "bool",
            "required": false,
            "default": false
        },
        {
            "name": "remark",
            "comment": "勋章消息的显示信息",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "grantTime",
            "comment": "勋章获取时间",
            "type": "int",
            "required": false,
            "default": 0
        }
    ]
}
