{
    "author": "wangjiantong",
    "comment": "按照分类id取得模板列表",
    "namespace": "NCFGroup\\Protos\\Contract",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
      {
        "name": "errorCode",
        "comment": "错误码(0为成功调用)",
        "type": "int",
        "required": true
      },
      {
        "name": "errorMsg",
        "comment": "错误信息",
        "type": "string",
        "required": true
      },
      {
        "name": "list",
        "comment": "模板列表",
        "type": "array",
        "required": false,
        "default":null
      },
      {
        "name": "count",
        "comment": "总记录数",
        "type": "int",
        "required": false,
        "default":null
      }
    ]
}
