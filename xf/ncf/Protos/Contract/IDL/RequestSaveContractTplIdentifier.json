{
    "author": "fanjingwen",
    "comment": "保存合同模板标识",
    "namespace": "NCFGroup\\Protos\\Contract",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "id",
            "comment": "模板标识 id",
            "type": "int",
            "default": 0
        },
        {
            "name": "name",
            "comment": "模板标识 名称",
            "type": "string",
            "required": true
        },
        {
            "name": "title",
            "comment": "模板标识 标题",
            "type": "string",
            "required": true
        },
        {
            "name": "signRole",
            "comment": "签署方",
            "type": "int",
            "required": true
        },
        {
            "name": "platformUserId",
            "comment": "平台用户id(用于盖章)",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "contractSendNode",
            "comment": "签署方",
            "type": "int",
            "required": true
        },
        {
            "name": "isSeenWhenBid",
            "comment": "是否投资时（用户）可见，0：否；1：是",
            "type": "int",
            "required": true
        },
        {
            "name": "serviceType",
            "comment": "服务类型：1：标的；2：项目",
            "type": "int",
            "required": true
        },
        {
            "name": "contractType",
            "comment": "合同类型，如：借款合同、委托协议",
            "type": "int",
            "required": true
        }
    ]
}
