{
    "author": "fanjingwen",
    "comment": "保存对应标的的合同附件路径",
    "namespace": "NCFGroup\\Protos\\Contract",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
      {
        "name": "dealId",
        "comment": "标的的id",
        "type": "int",
        "required": true
      },
      {
        "name": "jsonData",
        "comment": "合同附件地址的json格式信息",
        "type": "string",
        "required": true
      },
      {
        "name": "sourceType",
        "comment": "来源类型(0:P2P,1:通知贷,2:交易所,3:专享)",
        "type": "int",
        "required": false,
        "default": 0
      }
    ]
}
