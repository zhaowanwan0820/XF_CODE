{
    "author": "wangjiantong",
    "comment": "按照标的ID获取模板列表",
    "namespace": "NCFGroup\\Protos\\Contract",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
      {
        "name": "dealId",
        "comment": "标的ID",
        "type": "int",
        "required": true
      },
      {
        "name": "type",
        "comment": "类型(0:p2p;1:多投宝)",
        "type": "int",
        "required": false,
        "default":0
      },
      {
        "name": "sourceType",
        "comment": "来源类型(0:P2P,1:通知贷,2:交易所,3:专享)",
        "type": "int",
        "required": false,
        "default": 0
      },
      {
        "name": "time",
        "comment": "如果是多投宝，则可以传用户投资时间",
        "type": "int",
        "required": false,
        "default": 0
      }
    ]
}
