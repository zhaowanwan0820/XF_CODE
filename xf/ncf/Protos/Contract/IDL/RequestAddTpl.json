{
    "author": "wangjiantong",
    "comment": "添加模板",
    "namespace": "NCFGroup\\Protos\\Contract",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
      {
        "name": "contractTitle",
        "comment": "模板标题",
        "type": "string",
        "required": true
      },
      {
        "name": "name",
        "comment": "模板名称",
        "type": "string",
        "required": true
      },
      {
        "name": "contractCid",
        "comment": "模板分类ID",
        "type": "int",
        "required": true
      },
      {
        "name": "content",
        "comment": "模板内容",
        "type": "string",
        "required": true
      },
      {
        "name": "type",
        "comment": "模板类型",
        "type": "int",
        "required": false,
        "default":1
      },
      {
        "name": "isHtml",
        "comment": "是否html模板",
        "type": "int",
        "required": false,
        "default":1
      },
      {
        "name": "version",
        "comment": "合同版本号",
        "type": "float",
        "required": false,
        "default":1.0
      },
      {
        "name": "tplIdentifierId",
        "comment": "关联模板标识 id",
        "type": "int",
        "required": true
      }

    ]
}
