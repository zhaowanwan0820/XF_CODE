        },
        "1": {
            "required": true,
            "name": "amount",
            "type": "int",
            "comment": "申购总额（单位：分）"
        },
        "2": {
            "required": true,
            "name": "code",
            "type": "string",
            "comment": "基金代码"
        },
        "3": {
            "required": false,
            "name": "arriveTimeDes",
            "type": "array",
            "comment": "到账时间说明",
