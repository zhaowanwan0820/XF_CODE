{
    "namespace": "NCFGroup\\Protos\\FundGate",
    "comment": "向基金公司发起申购",
    "author": "Gu Weigang <guweigang@ucfgroup.com>",
    "use": {
        "0": "NCFGroup\\Common\\Extensions\\Base\\ResponseBase",
        "1": "Assert\\Assertion"
    },
    "extend": "ResponseBase",
    "properties": {
        "0": {
            "required": true,
            "name": "name",
            "type": "string",
