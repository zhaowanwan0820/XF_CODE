{
    "author": "yutao",
    "comment": "获取用户领取券码列表",
    "namespace": "NCFGroup\\Protos\\O2O",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\AbstractRequestBase", "NCFGroup\\Common\\Extensions\\Base\\Pageable", "Assert\\Assertion"],
    "extend": "AbstractRequestBase",
    "properties": [
        {
            "name": "userId",
            "comment": "用户ID",
            "type": "int",
            "required": true
        },
        {
            "name": "status",
            "comment": "券码状态",
            "type": "int",
            "required": true
        },
        {
            "name": "page",
            "comment": "页码",
            "type": "int",
            "required": false,
            "default": 1
        },
        {
            "name": "pageSize",
            "comment": "每页显示",
            "type": "int",
            "required": false,
            "default": 10
        },
        {
            "name": "hasTotalCount",
            "comment": "是否包含记录总数",
            "type": "int",
            "required": false,
            "default": 1
        }
    ]
}
