{
    "author": "yanbingrong",
    "comment": "获取可用的优惠券",
    "namespace": "NCFGroup\\Protos\\O2O",
    "use":["NCFGroup\\Common\\Extensions\\Base\\AbstractRequestBase","NCFGroup\\Common\\Extensions\\Base\\Pageable", "Assert\\Assertion"],
    "extend": "AbstractRequestBase",
    "properties":[
        {
            "name": "ownerUserId",
            "comment": "用户id",
            "type": "int",
            "required": true
        },
        {
            "name": "bidAmount",
            "comment": "投资金额",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "bidDayLimit",
            "comment": "投资期限",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "projectId",
            "comment": "项目id",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "category",
            "comment": "产品类别",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "dealTag",
            "comment": "标tag",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "page",
            "comment": "页码",
            "type": "int",
            "required": false,
            "default": 1
        },
        {
            "name": "pageSize",
            "comment": "每页显示",
            "type": "int",
            "required": false,
            "default": 10
