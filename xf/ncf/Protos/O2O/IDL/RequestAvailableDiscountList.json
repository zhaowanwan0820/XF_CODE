        {
            "name": "annualizedAmount",
            "comment": "年化投资额",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "consumeType",
            "comment": "交易类型，1为p2p，2为duotou，3为gold",
            "type": "int",
            "required": false,
            "default": 1
        }
