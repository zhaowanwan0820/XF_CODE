{
    "author": "liguizhi",
    "comment": "o2o:向合作方获取",
    "namespace": "NCFGroup\\Protos\\O2O",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "clientId",
            "comment": "网信给合作方分配的唯一key",
            "type": "string",
            "required": true
        },
        {
            "name": "couponNumber",
            "comment": "券码",
            "type": "string",
            "required": true
        },
        {
            "name": "couponId",
            "comment": "优惠券Id",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "userId",
            "comment": "用户编号",
            "type": "string",
            "required":false,
            "default" : ""
        },
        {
            "name": "productId",
            "comment": "合作方商品编号",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "extra",
            "comment": "合作方其他信息",
            "type": "string",
            "required": false,
            "default": ""
        }
    ]
}
