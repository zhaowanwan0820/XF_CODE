        {
            "name": "wxUserId",
            "comment": "出资方id",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "productName",
            "comment": "券码名称",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "goodsMaxPrice",
            "comment": "最大返利金额",
            "type": "float",
            "required": false,
            "default": 0.0
        },
        {
            "name": "goodsGiveType",
            "comment": "加息券返利方式，1为随息发放，2为一次发放",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "type",
            "comment": "投资券类型，1为返现券，2为加息券",
            "type": "int",
            "required": false,
            "default": 1
        }
    ]
}
