{
    "author": "yanbingrong",
    "comment": "兑换优惠券返回信息",
    "namespace": "NCFGroup\\Protos\\O2O",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "id",
            "comment": "券码ID",
            "type": "int",
            "required": true
        },
        {
            "name": "discountGroupId",
            "comment": "券组ID",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "ownerUserId",
            "comment": "领取人ID",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "status",
            "comment": "使用状态",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "useStartTime",
            "comment": "使用开始时间",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "useEndTime",
            "comment": "使用结束时间",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "createTime",
            "comment": "领取时间",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "updateTime",
            "comment": "最后修改时间",
            "type": "int",
            "required": false,
            "default": ""
        },
        {
            "name": "goodsType",
            "comment": "返利类型",
            "type": "int",
            "required": false,
            "default": 0
        },
        {
            "name": "goodsPrice",
            "comment": "返利金额",
            "type": "float",
            "required": false,
            "default": 0.0
