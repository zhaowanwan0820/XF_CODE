{
    "author": "liguizhi",
    "comment": "o2o:向合作方推送订单信息",
    "namespace": "NCFGroup\\Protos\\O2O",
    "use": ["NCFGroup\\Common\\Extensions\\Base\\ProtoBufferBase", "Assert\\Assertion"],
    "extend": "ProtoBufferBase",
    "properties": [
        {
            "name": "orderId",
            "comment": "优惠券兑换订单",
            "type": "string",
            "required": true
        },
        {
            "name": "clientId",
            "comment": "网信给合作方分配的唯一key",
            "type": "string",
            "required": true
        },
        {
            "name": "productId",
            "comment": "合作方商品编号",
            "type": "string",
            "required": true
        },
        {
            "name": "receiverName",
            "comment": "收货人姓名",
            "type": "string",
            "required": true
        },
        {
            "name": "receiverPhone",
            "comment": "收货人电话",
            "type": "string",
            "required": true
        },
        {
            "name": "receiverAddress",
            "comment": "详细收货地址",
            "type": "string",
            "required": true
        },
        {
            "name": "receiverCode",
            "comment": "收货邮编",
            "type": "string",
            "required":false,
            "default" : ""
        },
        {
            "name": "receiverArea",
            "comment": "收货省市区",
            "type": "string",
            "required": false,
            "default": ""
        },
        {
            "name": "receiverExtra",
            "comment": "订单其他信息",
            "type": "string",
            "required": false,
            "default": ""
        }
    ]
}
